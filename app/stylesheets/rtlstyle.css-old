body {
    direction:rtl;
    font-size: larger;
	}
#banner .banner_site-title {
    float:right;
    margin-right: 50px;
}
#user_locale_switcher {
    float:left;
}
#navigation_search {
    right: auto;
    left: 0;
}
.homepage-hero-content-wrapper {
    float: left;
    margin-right: 16.66667% !important;
}
.intro__browse {
    float: left;
}
.hero__intro {
    float: right;
}
.navigation a {
    border-right: none;
    border-left: 1px solid #33c5ed;
    border-width: 2px;
}
.navigation .account-link-menu-item a {
    border-left: none;
}
.steps__list {
    direction: rtl;
}
.steps__list li {
    float: right;
}

.steps__step-number {
    background-color: #f3f1eb;
    color:#0056a6;
    left: 75%;
    top: 9.5em;
    box-shadow: 0 1px 2px rgb(0, 0, 0);
    padding: 0.4em;
    height: 2.2em;
    width: 2.2em;
}
.p_howitwork {
    margin-top:1.5em;
}
.homepage-hero .new-request__make-new-requests{
    font-size: xx-large;
}
#left_column_flip.left_column_flip--opposite{
    float: left;
}
#right_column_flip, #right_column{
    float: left;
}
.body-listing__header {
    margin-right: unset;
    margin-left: 7em;
}
.body-listing__description {
    margin-right: unset;
    margin-left: 11.25em;
}
#header_left{
    float: right;
}















